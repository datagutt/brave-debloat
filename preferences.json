  "dashboard": {
    "show_clock": true,
    "show_background_image": false,
    "show_stats": false,
    "show_shortcuts": false,
    "show_branded_background_image": false,
    "show_cards": false,
    "show_search_widget": false,
    "show_brave_news": false,
    "show_together": false
  },
  "experimental_features": [
    "brave-adblock-experimental-list-default@1"
  ]
